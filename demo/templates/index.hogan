<!doctype html>
<html>
    <head>
        <link rel="stylesheet" href="demo.css">
    </head>
    
    <body>
        {{#schemes}}
        <div class='scheme'>
            <h1>$scheme: '{{scheme}}', $variation: '{{variation}}'</h1>
            {{#colors}}
            <div class='row'>${{color}}
                {{#variants}}
                <div class='box {{scheme}}-{{color}}{{variant}}'></div>
                {{/variants}}
            </div>
            {{/colors}}
        </div>
        {{/schemes}}
    </body>
</html>